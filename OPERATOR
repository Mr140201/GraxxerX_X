#include<iostream>
using namespace std;
int main ()
{
    int x;
    int y;
    int z;

    cout << "Masukkan Nilai A  = ";
    cin >> x ;
    cout << "Masukkan Nilai B  = ";
    cin >> y ;
    cout << "Masukkan Nilai C  = ";
    cin >> z;

    cout <<"\n hasil dari masukkan adalah : " << endl<< endl;
    cout <<" A or B = " << ( x || y ) << endl;
    cout <<" A or C = " << ( x || z ) << endl;
    cout <<" F1=(A or B} AND (A or C) = " << ((x || y) && (x || z)) << endl;


    return 0;


}
